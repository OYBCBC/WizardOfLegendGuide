<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oybc.wizardoflegendguide">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.ArcanaShowActivityTest"></activity>
        <activity android:name=".ui.activity.ArcanaDetailActivity"></activity>
        <activity android:name=".ui.activity.RelicsShowActivityTest"></activity>
        <activity android:name=".ui.activity.RelicsDetailActivity"></activity>
        <activity android:name=".ui.activity.CloakShowActivityTest"></activity>
        <activity android:name=".ui.activity.CloakDetailActivity"></activity>

    </application>

</manifest>